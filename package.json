{
  "name": "timeward",
  "version": "0.8.0",
  "description": "productivity for perfectionists",
  "main": "index.js",
  "repository": "git@github.com:rpivo/timeward.git",
  "author": "Ryan Pivovar <ryanpivovar@gmail.com>",
  "license": "MIT",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "moduleNameMapper": {
      "@components/(.*)": "<rootDir>/src/components/$1",
      "@pages/(.*)": "<rootDir>/src/pages/$1",
      "@styles/(.*)": "<rootDir>/src/styles/$1",
      "@utilities/(.*)": "<rootDir>/src/utilities/$1"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/spec/setupTests.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/eslint": "^6.1.4",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.18",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.4.2",
    "@types/webpack": "^4.41.3",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.7.1",
    "eslint-plugin-react": "^7.18.0",
    "jest": "^25.1.0",
    "react-test-renderer": "^16.12.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^3.2.6",
    "aws-sdk": "^2.648.0",
    "docz": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.0"
  },
  "scripts": {
    "analyze": "webpack --config webpack/webpack.analyzer.ts --progress",
    "build": "yarn lint && webpack --config webpack/webpack.prod.ts --progress",
    "docs": "yarn docz dev",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "start": "webpack-dev-server --config webpack/webpack.dev.ts --progress",
    "test": "jest --watchAll --coverage --notify"
  }
}
